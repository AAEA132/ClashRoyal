<AnchorPane prefHeight="600.0" prefWidth="400.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.SignUpController">
   <children>
      <Label layoutX="172.0" layoutY="114.0" prefHeight="35.0" prefWidth="76.0" text="SIGN UP" textAlignment="CENTER">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="111.0" layoutY="159.0" prefHeight="35.0" prefWidth="200.0" promptText="Username" />
      <TextField fx:id="passField" layoutX="111.0" layoutY="206.0" prefHeight="35.0" prefWidth="200.0" promptText="Password" />
      <Button fx:id="signUpButton" layoutX="104.0" layoutY="360.0" mnemonicParsing="false" onAction="#actionHandler" prefHeight="35.0" prefWidth="212.0" text="Sign Up" />
      <Hyperlink layoutX="191.0" layoutY="462.0" text="Login" />
      <TextField fx:id="rePassField" layoutX="110.0" layoutY="253.0" prefHeight="35.0" prefWidth="200.0" promptText="Repeat Password" />
      <Button fx:id="loginLink" layoutX="174.0" layoutY="411.0" mnemonicParsing="false" onAction="#actionHandler" text="Login" />
      <Label fx:id="massageLable" layoutX="111.0" layoutY="311.0" text="Signed up successfully" />
   </children>
